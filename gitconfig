[user]
  name = Martin Jakubik
  email = martin@jakubik.info
[push]
  default = simple
[core]
  editor = vim
  excludesfile = /Users/chuckeles/.gitignore_global
[color]
  diff = auto
  ui = auto
[alias]
  a = add
  aa = add --all
  aaco = !git add --all && git commit
  aacoa = !git add --all && git commit --amend
  b = branch
  cl = clone
  co = commit
  coa = commit --amend
  count = !git ls-files | xargs wc -l
  ce = checkout
  cp = cherry-pick
  d = diff
  ds = diff --stat
  f = fetch
  l = log --graph --decorate --all --pretty=oneline --abbrev-commit
  m = merge
  mff = merge --ff-only
  mnff = merge --no-ff
  ps = push
  pl = pull
  plr = pull --rebase
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbi = rebase -i
  rs = reset
  rsh = reset --hard
  s = status

